{
  "name": "welive",
  "version": "1.0.0",
  "description": "Codeit1Team Project Welive",
  "main": "dist/core/index.js",
  "scripts": {
    "_section1": "Prisma",
    "seed": "ts-node -r tsconfig-paths/register src/prisma/seed.ts",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma && npm run seed",
    "prisma:reset": "prisma migrate reset --schema=prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma",
    "_section2": "Lint & Format",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write .",
    "_section3": "Build & Clean",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "_section4": "Test",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "_section5": "Development",
    "dev:be": "cross-env NODE_ENV=development nodemon --watch src --ext ts --exec \"ts-node -r tsconfig-paths/register src/core/index.ts\"",
    "dev:fe": "cross-env NODE_ENV=development npm run dev --prefix frontend",
    "dev": "concurrently \"npm run dev:be\" \"npm run dev:fe\"",
    "_section6": "Production",
    "start:be": "cross-env NODE_ENV=production pm2 start dist/core/index.js --name welive-be",
    "start:fe": "cross-env NODE_ENV=production pm2 start npm --name welive-fe -- run start --prefix frontend",
    "start": "npm run start:be && npm run start:fe",
    "stop": "pm2 delete welive-be || true && pm2 delete welive-fe || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeit-welive/welive.git"
  },
  "author": "codeit1team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeit-welive/welive/issues"
  },
  "homepage": "https://github.com/codeit-welive/welive#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@aws-sdk/s3-request-presigner": "^3.911.0",
    "@prisma/client": "^6.17.1",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "file-type": "^21.0.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "sharp": "^0.34.4",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "imports": {
    "#core/*": "./src/core/*",
    "#modules/*": "./src/modules/*",
    "#constants/*": "./src/common/constants/*",
    "#errors/*": "./src/common/errors/*",
    "#helpers/*": "./src/common/helpers/*",
    "#aws/*": "./src/core/aws/*",
    "#middlewares/*": "./src/core/middlewares/*",
    "#utils/*": "./src/core/utils/*",
    "#sse/*": "./src/core/sse/*",
    "#crons/*": "./src/crons/*"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
